/**
 * @param {number[]} arr
 * @param {Function} fn
 * @return {number[]}
 */


var filter = function(arr, fn) {
     
      var newar =[];
      var c=0;
      for(var i=0;i<arr.length;i++)
      {
           if(fn(arr[i],i))
           {
                 newar[c++] = arr[i];
           }
            
      }

      return newar;
};
